{
  "name": "waywedo-htmldiff-js",
  "version": "1.1.0",
  "description": "JavaScript port of HtmlDiff.Net which is itself a C# port of HtmlDiff. Updated for Way We Do",
  "main": "./dist/htmldiff.min.js",
  "author": "Jordan Hitch / Way We Do",
  "license": "ISC",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waywedo/htmldiff-js"
  },
  "keywords": [
    "HtmlDiff",
    "Html",
    "Diff",
    "Diff Algorithm",
    "Diff Structured File"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./jestsetup.js"
    ],
    "testPathIgnorePatterns": [
      "/src/",
      "/dist/"
    ],
    "transform": {
      "^(?:.)+\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!waywedo-datepicker)"
    ]
  },
  "browserslist": [
    "supports es6 and >0.5%",
    "not dead"
  ],
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "babel-jest": "^29.5.0",
    "clean-webpack-plugin": "^4.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "unminified-webpack-plugin": "^3.0.0",
    "webpack": "^5.86.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4"
  }
}