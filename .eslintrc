{
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "plugins": [],
    "env": {
        "amd": false,
        "browser": true,
        "jquery": false,
        "node": true,
        "es6": true,
        "worker": false
    },
    "rules": {
        "curly": 1,
        "eqeqeq": 2,
        "no-console": 1,
        "no-debugger": 1,
        "no-irregular-whitespace": 1,
        "no-undef": 2,
        "no-unused-vars": 2,
        "no-var": 1,
        "valid-jsdoc": [
            2,
            { "requireReturn": false }
        ]
    }
}